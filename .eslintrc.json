{
    "env": {
        "es6": true,
        "browser": true
    },
    "globals": {
        "require": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 8
    },
    "rules": {
        "no-unused-vars": "off",
        "no-redeclare": "off",
        "no-undef": "off"
    }
}
